@model CMS.MODEL.Addmission.AddmissionEditModel

@{
    ViewBag.Title = "GenderList";
    Layout = null;
}

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <div class="form-group">
            <div class="col-md-10">
                @Html.DropDownListFor(model => model.GenderGuid, new SelectList(ViewBag.GenderList, "GenderGuid", "Gender"), "Select Gender", new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.GenderGuid, "", new { @class = "text-danger" })
            </div>
        </div>
    </div>
}